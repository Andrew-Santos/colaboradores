{
  "version": 2,
  "functions": {
    "api/server.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/auth/:path*",
      "destination": "/api/server"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/server"
    },
    {
      "source": "/health",
      "destination": "/api/server"
    }
  ]
}
